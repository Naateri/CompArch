#include <iostream>
using namespace std;

void printALU(){
	cout<<" "<<endl;
	cout<<"  -------------------------------------------------------------------------------"  <<endl;
	cout<<" |                                _______                                        |" <<endl;
	cout<<" |                               |       \\                                       |"  <<endl;
	cout<<" |         _______               |        \\           Guardando en rd:           |"  <<endl;
	cout<<" |        |       |              |         \\             ____________            |"  <<endl;
	cout<<" |        |  Reg  |  ---->rs:    |          |           |            |           |"  <<endl;
	cout<<" | |      |       |              |   ALU    |   ----->  | Data Cache |----       |"  <<endl;
	cout<<" | |      |  File |  ---->rt:    |          |           |____________|    |      |"  <<endl;
	cout<<" | |  --> |_______|    |         |         /                              |      |"  <<endl;
	cout<<" | | |                 | imm     |        /                               |      |"  <<endl;
	cout<<" |  -+-----------------          |_______/                                |      |"  <<endl;
	cout<<" |   |                                                                    |      |"  <<endl;
	cout<<" |    --------------------------------------------------------------------       |"  <<endl;
	cout<<" |                                                                               |"  <<endl;
	cout<<" |                                                                               |"  <<endl;
	cout<<" |                 .........................................                     |"  <<endl;
	cout<<" |                 :                                       :                     |"  <<endl;
	cout<<" |                 :               CONTROL                 :                     |"  <<endl;
	cout<<" |                 :                                       :                     |"  <<endl;
	cout<<" |                 :.......................................:                     |"  <<endl;
	
	cout<<" |                                                                               |"  <<endl;
	cout<<" |                                                                               |"  <<endl;
	cout<<"  -------------------------------------------------------------------------------"  <<endl;
	
}

int main(){
	printALU();
	
	
	return 0;
}
